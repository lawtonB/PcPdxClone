@{
    ViewData["Title"] = "Log In";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model IEnumerable<PcPdx.Models.ApplicationUser>

<h1>UserList</h1>
<ul>
    @foreach (var user in Model)
    {
        <li>@Html.DisplayFor(ModelShow => user.UserName) | @Html.ActionLink("Edit User", "Edit", "Account", new { id = user.Id }) | @Html.ActionLink("Delete User", "Delete", "Account", new { id = user.Id })</li>

    }
</ul>

